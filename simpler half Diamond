#include <iostream>
using namespace std;

int main() {
    int size = 15;

    // Top half of the diamond
    for (int i = 1; i <= size; i++) {
        cout << string(i, '*') << endl; // Print 'i' asterisks
    }

    // Bottom half of the diamond
    for (int i = size - 1; i > 0; i--) {
        cout << string(i, '*') << endl; // Print 'i' asterisks
    }

    return 0;
}
