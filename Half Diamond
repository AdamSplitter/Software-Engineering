#include <iostream>
#include <vector>
using namespace std;

int main(){
    vector<string> x = {"*"};
    int size = 15;

    for (int i=0; i < size; i++){
        for (const string & star :x){
            cout << star;
        }
        cout << endl;
        x.push_back("*");
    }
    x = {"*"};

    for (int i =0; i <= size; i++){
      x.push_back("*");
    }

    for (int i=0; i < size; i++){
        for (const string & star :x){
            cout << star;
        }
        cout << endl;
        x.pop_back();
    }
}
